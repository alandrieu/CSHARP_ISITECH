<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                  assembly="CaveLib"
                  namespace="CaveLib.Bean">

  <class name="Bouteille" table="bouteilles">
    <id name="IdBouteille">
      <generator class="guid" />
    </id>

    <property name="Name" />
    <property name="Category" />
    <property name="PrixU" />
  </class>

</hibernate-mapping>